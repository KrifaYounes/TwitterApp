<faces-config xmlns="http://java.sun.com/xml/ns/javaee"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"	version="2.0">		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginSuccess</from-outcome>			<to-view-id>/app/main.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginFailed</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<navigation-case>			<from-action>#{loginManagedBean.logout}</from-action>			<from-outcome>logoutSuccess</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>	<navigation-rule>		<display-name>login/register.xhtml</display-name>		<from-view-id>/login/register.xhtml</from-view-id>		<navigation-case>			<from-outcome>successCreate</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>	<navigation-rule>		<display-name>login/register.xhtml</display-name>		<from-view-id>/login/register.xhtml</from-view-id>		<navigation-case>			<from-outcome>failedCreate</from-outcome>			<to-view-id>/login/register.xhtml</to-view-id>		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>app/profil.xhtml</display-name>		<from-view-id>/app/profil.xhtml</from-view-id>		<navigation-case>			<from-outcome>successUpdate</from-outcome>			<to-view-id>/app/main.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>	<navigation-rule>		<display-name>app/profil.xhtml</display-name>		<from-view-id>/app/profil.xhtml</from-view-id>		<navigation-case>			<from-outcome>failedUpdate</from-outcome>			<to-view-id>/app/profil.xhtml</to-view-id>		</navigation-case>	</navigation-rule>
</faces-config>